public static void main(String[] args) {
        java.util.Scanner scan = new java.util.Scanner(System.in);
        int N;
        System.out.print("NÃºmero da tabuada: ");
        N = scan.nextInt();
        for (int i = 1; i <= 10; i++) {
        System.out.print(i+" x "+N+" = "+(i*N)+"\n");
        }
  
    }

}
